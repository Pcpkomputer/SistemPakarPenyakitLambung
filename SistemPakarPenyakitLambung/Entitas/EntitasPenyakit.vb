Public Class EntitasPenyakit
    Private idpenyakit, namapenyakit, penanganan As String

    Public Property idpenyakit_
        Get
            Return idpenyakit
        End Get
        Set(value)
            idpenyakit = value
        End Set
    End Property

    Public Property namapenyakit_
        Get
            Return namapenyakit
        End Get
        Set(value)
            namapenyakit = value
        End Set
    End Property

    Public Property penanganan_
        Get
            Return penanganan
        End Get
        Set(value)
            penanganan = value
        End Set
    End Property

End Class
